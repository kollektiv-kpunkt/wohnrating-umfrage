<?php

$parties = [
    "sp",
    "svp",
    "gruene",
    "fdp",
    "glp",
    "cvp",
    "bdp",
    "mitte",
    "evp"
];

$file = fopen('partyhashes.csv', 'w');

for ($x = 0; $x < count($parties); $x++) {
    $hash = hash("sha3-224", rand(5000, 10000));
    fputcsv($file, array($x + 1, $parties[$x], $hash));
}

fputcsv($file, array($x + 1, "testing", 1));

fclose($file);


?>